from std use (sin, abs)

DELTA :: 0.00000000001
PI :: 3.141592653589793238462643383279

start :: fn do
    abs(sin(0.0) - 0.0) < DELTA <=> true
    abs(sin(PI) - 0.0) < DELTA <=> true
    abs(sin(PI / 2.0) - 1.0) < DELTA <=> true
    abs(sin(3.0 * PI / 2.0) + 1.0) < DELTA <=> true
    abs(sin(2.0 * PI) - 0.0) < DELTA <=> true
    abs(sin(3.0 * PI) - 0.0) < DELTA <=> true
end
