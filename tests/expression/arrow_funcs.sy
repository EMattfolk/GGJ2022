from list use (push)

l := []
f :: fn a: int, b: int -> int do
    l -> push(b)
    ret a + 1
end

g :: fn a: int -> bool do
    l -> push(a)
    ret true
end

start :: fn do
    l = []
    1 -> f(1) -> f(2) -> f(3)
    l <=> [1, 2, 3]
    l = []
    1 -> f(1) -> f(2) -> f(3) -> g()
    l <=> [1, 2, 3, 4]
    l = []
    1 -> g()
    print(l)
    l <=> [1]
    l = []
    g(f(3, 2 -> f(1 -> f(1))))
    l <=> [1, 2, 3, 4]
end

